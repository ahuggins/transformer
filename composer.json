{
  "name": "ahuggins/transformer",
  "description": "A form generation package for Laravel",
  "keywords": [],
  "license": "",
  "type": "library",
  "authors": [
    {
      "name": "Andrew Huggins",
      "email": "andrewhuggins@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/contracts": "5.5.*",
    "illuminate/support": "5.5.*"
  },
  "require-dev": {
    "graham-campbell/testbench": "^3.1",
    "mockery/mockery": "^0.9.4",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Ahuggins\\Transformer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ahuggins\\Tests\\Transformer\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
        "providers": [
            "Ahuggins\\Transformer\\TransformerServiceProvider"
        ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
